# Open Notebook
OPEN_NOTEBOOK_BASE_URL=http://192.168.2.185:5055
OPEN_NOTEBOOK_SEARCH_PATH=/api/search
OPEN_NOTEBOOK_TIMEOUT_SECONDS=25

# Retrieval baseline (兜底)
BASELINE_SEARCH_TYPE=vector
BASELINE_LIMIT=8
BASELINE_MIN_SCORE=0.58
BASELINE_SEARCH_SOURCES=true
BASELINE_SEARCH_NOTES=false

# Trigger behavior
GROUP_REQUIRE_ADDRESSED=true
ENABLE_INTENT_GATE=true

# CORS（如需从局域网网页/跨域前端直接访问中间层）
# 逗号分隔；默认 *
CORS_ALLOW_ORIGINS=*

# LLM routing (non-紫微直连，紫微用于证据综合)
ENABLE_LLM=true
LLM_BASE_URL=http://192.168.2.195:3000/openai-codex-oauth/v1
LLM_API_KEY=123456
LLM_MODEL=gpt-5.3-codex
LLM_TIMEOUT_SECONDS=40

# Chart engine (must-use)
IZTRO_SCRIPT_PATH=/app/scripts/iztro_chart.mjs
IZTRO_TIMEOUT_SECONDS=12

# 可视化分享（由中间层统一生成 + 统一过期提示）
ENABLE_VIS_SHARE=true
VIS_SHARE_API_URL=https://zw.120323.xyz/api/share
VIS_SHARE_PUBLIC_BASE=https://zw.120323.xyz
VIS_SHARE_TTL_SECONDS=7200
VIS_SHARE_TIMEOUT_SECONDS=12
